(define (problem final)
    (:domain agenteSeguranca)
    (:objects Corredor 
              Sala1 Porta1 Janela1
              Sala2 Porta2 Janela2
              Sala3 Porta3 Janela3
              Sala4 Porta4 Janela4
              Sala5 Porta5 Janela5
              Sala6 Porta6 Janela6)
    (:init 
      (Sala Corredor) (Em Corredor)
      (Sala Sala1)
      (Porta Porta1) (Pertence Sala1 Porta1) (Pertence Corredor Porta1)
      (Janela Janela1) (Pertence Sala1 Janela1)
      (Sala Sala2)
      (Porta Porta2) (Pertence Sala2 Porta2) (Pertence Sala1 Porta2)
      (Janela Janela2) (Pertence Sala2 Janela2)
      (Sala Sala3)
      (Porta Porta3) (Pertence Sala3 Porta3) (Pertence Sala2 Porta3)
      (Janela Janela3) (Pertence Sala3 Janela3)
      (Sala Sala4)
      (Porta Porta4) (Pertence Sala4 Porta4) (Pertence Corredor Porta4)
      (Janela Janela4) (Pertence Sala4 Janela4)
      (Sala Sala5)
      (Porta Porta5) (Pertence Sala5 Porta5) (Pertence Sala4 Porta5)
      (Janela Janela5) (Pertence Sala5 Janela5)
      (Sala Sala6)
      (Porta Porta6) (Pertence Sala6 Porta6) (Pertence Sala5 Porta6)
      (Janela Janela6) (Pertence Sala6 Janela6)
      (not (Aberta Porta1)) (Aberta Janela1)
      (not (Aberta Porta2)) (Aberta Janela2)
      (not (Aberta Porta3)) (Aberta Janela3)
      (not (Aberta Porta4)) (Aberta Janela4)
      (not (Aberta Porta5)) (Aberta Janela5)
      (not (Aberta Porta6)) (Aberta Janela6)
      (Conectadas Corredor Sala1)
      (Conectadas Sala1 Sala2)
      (Conectadas Sala2 Sala3)
      (Conectadas Corredor Sala4)
      (Conectadas Sala4 Sala5)
      (Conectadas Sala5 Sala6)
    )
    (:goal (and (not(Aberta Janela1)) (not(Aberta Porta1))
                (not(Aberta Janela2)) (not(Aberta Porta2))
                (not(Aberta Janela3)) (not(Aberta Porta3))
                (not(Aberta Janela4)) (not(Aberta Porta4))
                (not(Aberta Janela5)) (not(Aberta Porta5))
                (not(Aberta Janela6)) (not(Aberta Porta6))))
)
